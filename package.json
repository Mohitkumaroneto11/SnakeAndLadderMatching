{
  "name": "ludocontest",
  "version": "1.0.0",
  "description": "Contest managment service for ludo",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json && tsc-alias -p tsconfig.json && npm run cp",
    "cp": "cpx ./src/certs/jwt-token.pem dist/certs/",
    "preserve": "npm run build",
    "serve": "concurrently \"tsc --watch\" \"nodemon -q app.js\"",
    "prestart": "npm run build",
    "start": "node dist/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Puneet Jain",
  "license": "ISC",
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "amqplib": "^0.8.0",
    "bee-queue": "^1.4.0",
    "body-parser": "^1.20.0",
    "cpx": "^1.5.0",
    "express": "^4.18.1",
    "ioredis": "^5.0.3",
    "jsonwebtoken": "^8.5.1",
    "linq-to-typescript": "^10.0.0",
    "lodash": "^4.17.21",
    "log4js": "^6.4.6",
    "mssql": "^8.1.0",
    "multer": "^1.4.5-lts.1",
    "needle": "^3.1.0",
    "redis": "^4.0.6",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.4.1",
    "typescript-linq": "^1.0.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.7",
    "@types/mongoose": "^5.11.97",
    "@types/mssql": "^7.1.5",
    "@types/multer": "^1.4.7",
    "@types/needle": "^2.5.3",
    "@types/node": "^17.0.8",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.12.0",
    "lint-staged": "^12.1.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "tsc-alias": "^1.5.0",
    "typescript": "^4.5.4"
  }
}
